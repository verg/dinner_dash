<%= render "shared/cart_link" %>
<% cache(cache_key_for_categories) do %>
  <div id="category-links" class="col-sm-3 col-md-2 sidebar">
    <ul>
      <%= render @presenter.categories %>
    </ul>
  </div>
<% end %>

<% cache(cache_key_for_products) do %>
  <ul>
    <%= render 'products/tiles', products: @presenter.products %>
  </ul>
<% end %>
